package com.bankapp.jvabasics;

public class TC_4_Validatetheforloop {
	public static void main(String[] args) {

		

		/*
		 * for(int i=1;i<=20 ;i++) { if(i%2==0) { System.out.println("even number:"+i);
		 * } else { System.out.println("odd number:"+i); } }
		 * 
		 */
  
  
  for(int i=1;i<=10;i++) {
	  if(i==5) 
	  { continue; 
	  } 
	  System.out.println(i); } //i
  
	  // want to skip 3,5,9 for (int i = 1; i <= 10; i++) { if (i == 3 || i == 5 ||
	//  i == 9) { continue; } System.out.println(i); }
	 
	}
}