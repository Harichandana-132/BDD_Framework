package com.bankapp.jvabasics;

public class TC_4_Validatetheforloop {
	public static void main(String[] args) {

		
//even or odd
		for(int i=1;i<=20;i++)
		{
			if(i%2==0)
			{
				System.out.println("even number:"+i);
			}
			else
			{
				System.out.println("odd number:"+i);
			}
		}
		//skip 5
		for(int i=1;i<=20;i++)
		{
			if(i==5)
			{
				continue;
			}
			System.out.println(i);
		}
}
}























