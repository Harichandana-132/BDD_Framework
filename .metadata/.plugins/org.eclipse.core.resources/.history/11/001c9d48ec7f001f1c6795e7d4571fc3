package com.bankapp.jvabasics;

import java.util.Arrays;

public class TC_7_Validatethehetrogenousdatainarray {
	public static void main(String[] args) {
		
		Object a[]={"seenium","Tosca",2000,34.5f,'A'};
		//to read the single value from an array
		System.out.println(a[3]);
		//to read the multiple values from an array
		/*
		 * for(int i=0;i<=4;i++) { System.out.println(a[i]); }
		 */
		for(int i=0;i<a.length-1;i++)
		{
			System.out.println(a[i]);
		}
		
	}
	

}
