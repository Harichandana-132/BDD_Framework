package com.bankapp.jvabasics;
abstract class S1
{
	abstract void test1();
	abstract void test2();
}
class S2 extends S1
{
	void test1()
	{
		System.out.println("test 1 is running");
	}
	void test2()
	{
		System.out.println("test2 is running");
	}
}

public class TC_42_Validatetheabstractclass {

}
